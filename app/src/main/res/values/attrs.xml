<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircleDrawableListView">
        <attr name="maxCount" format="integer"/>
        <attr name="circlePadding" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="SlidingPanelViewGroup">
        <attr name="itemsCount" format="integer"/>
        <attr name="itemsTitle" format="enum"/>
        <attr name="itemsColor" format="color"/>
        <attr name="itemsTextColor" format="color"/>
        <attr name="itemsSelectedTextColor" format="color"/>
        <attr name="sliderColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="ShapeSelectorView">
        <attr name="shapeColor" format="color"/>
        <attr name="textColor" format="color"/>
        <attr name="showShapeName" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ArcAnimationView">
        <attr name="backgroundColor" format="color"/>
        <attr name="strokeWidth" format="dimension"/>
        <attr name="fillColor" format="color"/>
        <attr name="startAngle" format="integer"/>
    </declare-styleable>

</resources>